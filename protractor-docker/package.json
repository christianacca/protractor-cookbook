{
    "name": "protractor-docker-setup",
    "scripts": {
        "pretest": "yarn install && cd ../testapp && yarn install",
        "test": "docker-compose up --abort-on-container-exit --exit-code-from e2e",
        "start": "docker-compose run -d -p 8080:8080 angular",
        "stop": "docker-compose down -v"
    },
    "dependencies": {
        "jasmine-reporters": "^2.3.1",
        "protractor": "^5.4.0"
    }
}
